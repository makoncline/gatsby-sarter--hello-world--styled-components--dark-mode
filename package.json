{
  "name": "gatsby-starter-hello-world",
  "private": false,
  "description": "Bare bones gatsby starter with styled components and dark and light mode theme and toggle.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "develop": "npx --node-arg '-r esm' gatsby develop",
    "start": "npm run develop",
    "serve": "npx --node-arg '-r esm' gatsby serve",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "esm": "^3.2.25",
    "gatsby": "^2.23.10",
    "gatsby-plugin-styled-components": "^3.3.6",
    "gatsby-styled-components-dark-mode": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@iconify/icons-eva": "^1.0.0",
    "@iconify/icons-oi": "^1.0.5",
    "@iconify/react": "^1.1.3",
    "prettier": "2.0.5"
  }
}